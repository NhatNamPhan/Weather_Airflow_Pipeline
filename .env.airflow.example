# AIRFLOW CONFIGURATION
AIRFLOW_UID=50000
AIRFLOW_PROJ_DIR=.

# Airflow Web UI Credentials
_AIRFLOW_WWW_USER_USERNAME=admin
_AIRFLOW_WWW_USER_PASSWORD=your_password_here

# AIRFLOW METADATA DATABASE (PostgreSQL)
# Database lưu metadata của Airflow (DAGs, tasks, logs, etc.)
POSTGRES_HOST=host.docker.internal
POSTGRES_PORT=5433
POSTGRES_DB=airflow_weather
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_password_here

AIRFLOW__DATABASE__SQL_ALCHEMY_CONN=postgresql+psycopg2://postgres:your_password_here@host.docker.internal:5433/airflow_weather

# WEATHER APPLICATION DATABASE (PostgreSQL)
# Database lưu dữ liệu weather thực tế của project
DB_HOST=host.docker.internal
DB_PORT=5433
DB_NAME=weather_db
DB_USER=postgres
DB_PASSWORD=your_password_here

# ADDITIONAL PYTHON PACKAGES
# Thêm packages cần thiết cho DAGs
_PIP_ADDITIONAL_REQUIREMENTS=openmeteo-requests requests-cache retry-requests pandas numpy sqlalchemy psycopg2-binary python-dotenv
