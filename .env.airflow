# ============================================
# AIRFLOW CONFIGURATION
# ============================================
AIRFLOW_UID=50000
AIRFLOW_PROJ_DIR=.

# Airflow Web UI Credentials
_AIRFLOW_WWW_USER_USERNAME=admin
_AIRFLOW_WWW_USER_PASSWORD=admin

# ============================================
# AIRFLOW METADATA DATABASE (PostgreSQL)
# Database này lưu metadata của Airflow (DAGs, tasks, logs, etc.)
# ============================================
POSTGRES_HOST=host.docker.internal
POSTGRES_PORT=5433
POSTGRES_DB=airflow_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=1234
AIRFLOW__DATABASE__SQL_ALCHEMY_CONN=postgresql+psycopg2://postgres:1234@host.docker.internal:5433/airflow_db

# ============================================
# WEATHER APPLICATION DATABASE (PostgreSQL)
# Database này lưu dữ liệu weather thực tế của project
# ============================================
DB_HOST=host.docker.internal
DB_PORT=5433
DB_NAME=weather_db
DB_USER=postgres
DB_PASSWORD=1234

# ============================================
# ADDITIONAL PYTHON PACKAGES
# ============================================
# Uncomment và thêm packages cần thiết cho DAGs
# _PIP_ADDITIONAL_REQUIREMENTS=requests pandas sqlalchemy psycopg2-binary python-dotenv
